{
  "name": "Tab Garbage Collector and Archiver",
  "version": "1.1.25",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon_016.png",
    "48": "icons/icon_048.png",
    "128": "icons/icon_128.png"
  },
  "description": "Tab Garbage Collector extension closes tabs that have not been viewed for long time.",
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon_016.png",
    "default_title": "Tab Garbage Collector and Archiver",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "bookmarks",
    "chrome://favicon/"
  ]
}